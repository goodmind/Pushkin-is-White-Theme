{
  "type": "dark",
  "name": "Pushkin is Black",
  
  "tokenColors": [
    /* TYPES */
    {
      "name": "String",
      "scope": [
        "string.quoted"
        // "meta.template punctuation"
      ],
      "settings": {
        "foreground": "#fc3d7f" // "#fc3d7f" // "#c7254e" // "#E26A6A" // "#C0392B" // "#ff0068AA" // "#DCB777"
      }
    },
    {
      "name": "Quotes",
      "scope": [
        "string.quoted punctuation.definition.string"
        // "string.template punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#E08283" // "#c7254e"//"#E08283" // "#E87E04" // "#E87E04"
      }
    },
    {
      "name": "Sring template",
      "scope": [
        "string.template punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#8757ad"
      }
    },
    {
      "name": "Sring template quotes",
      "scope": [
        "string.template punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#c7254e"//"#fc3d7f"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#5cb4de" // "#5C97BF" //"#d35400"
      }
    },

    {
      "name": "Array braces",
      "scope": [
        "meta.array.literal meta.brace"
      ],
      "settings": {
        "foreground": "#A5C261" //"#FFB054"
      }
    },
    {
      "name": "Array literal",
      "scope": [
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#519F50"
      }
    },
    {
      "name": "Array literal punctuation",
      "scope": [
        "meta.array.literal punctuation"
      ],
      "settings": {
        "foreground": "#F9F9F9"
      }
    },
    {
      "name": "Object braces",
      "scope": [
        "meta.var meta.objectliteral punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#BC9458" //"#CC7833"
      }
    },
    {
      "name": "Object braces as func. arg",
      "scope": [
        "meta.function meta.objectliteral punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#BC9458"
      }
    },
    {
      "name": "Object literal",
      "scope": [
        "meta.object-literal"
      ],
      "settings": {
        "foreground": "#CC7833"//"#BC9458"
      }
    },
    {
      "name": "Object literal punctuation",
      "scope": [
        "meta.object-literal punctuation"
      ],
      "settings": {
        "foreground": "#F9F9F9"//"#BC9458"
      }
    },
    {
      "name": "Object literal as func. arg",
      "scope": [
        "meta.function variable"
      ],
      "settings": {
        "foreground": "#F9F9F9" //"#CC7833"
      }
    },

    /* Comment */
    {
      "name": "Comment",
      "scope": [
        "comment"
      ],
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#555555" //"#746D73"
      }
    },
    /* Keyword operators */
    {
      "name": "Keyword comparison",
      "scope": [
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#5cb4de" //"#8757ad"//"#708394"
      }
    },
    /* Keyword controls */
    {
      "name": "Keyword controls",
      "scope": [
        "keyword.control",
        "storage.control", // JS? Clojure!
        "storage.type",
        "keyword.operator.expression",
        "keyword.operator.new",
        "variable.language.this",
        // "support.type"
        "support.type.object"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#555555"//"#708394"
      }
    },
    /* Braces */
    {
      "name": "Braces",
      "scope": [
        "meta.brace",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.terminator",
        "punctuation.accessor"
      ],
      "settings": {
        "foreground": "#555555"
      }
    },



    /* Clojure */
    {
      "name": "Vector",
      "scope": [
        "meta.vector punctuation.section.vector"
      ],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "Map",
      "scope": [
        "meta.map punctuation.section.map"
      ],
      "settings": {
        "foreground": "#BC9458"
      }
    },
    {
      "name": "Clojure - Level 0",
      "scope": [
        "source.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Clojure - Level 1",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Clojure - Level 2",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Clojure - Level 3",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Clojure - Level 4",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#C17E70"
      }
    },
    {
      "name": "Clojure - Level 5",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Clojure - Level 6",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Clojure - Level 7",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Clojure - Level 8",
      "scope": [
        "source.clojure meta.expression.clojure meta.definition.global.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure meta.expression.clojure punctuation.section.expression"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    }





