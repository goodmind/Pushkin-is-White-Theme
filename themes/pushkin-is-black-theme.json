  "colors": {
    /* Search */
    "editor.findMatchBackground": "#A03000",
    // "editor.findMatchHighlightBackground": "#000F8090",
    // "editor.findRangeHighlightBackground": "#ffff00",
    /* Editor */
    "titleBar.activeBackground": "#18181A",
    "titleBar.activeForeground": "#ffffff90",
    "titleBar.inactiveForeground": "#ffffff90",
    "titleBar.inactiveBackground": "#18181A",
    // "titleBar.border": "#000000",
    "focusBorder": "#746D73",

    "editorGutter.addedBackground": "#ffffff50",

    "editor.background": "#1C1C1C",
    "editor.foreground": "#F9F9F9",//"#bdc3c7", // A BIT DARK
    // "editorLineNumber.foreground": "#75715E",
    "editorCursor.foreground": "#B6B6B6",
    "editor.selectionBackground": "#000000F0",
    "editor.lineHighlightBackground": "#00000090",
    // "editor.selectionHighlightBackground": "#362712",

    /* Workbench: activitybar */
    "activityBar.background": "#1A1A1A", //"#000000",
    "activityBar.foreground": "#bdc3c7",
    "activityBarBadge.background": "#bdc3c7",
    "activityBarBadge.foreground": "#000000",

    /* Workbench: sidebar */
    "sideBar.background": "#1A1A1A",
    "sideBar.foreground": "#ffffff50",
    // "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#1A1A1A",
    // "sideBarSectionHeader.foreground": "#ffffff",

    "list.activeSelectionBackground": "#ffffff10",
    "list.inactiveSelectionBackground": "#ffffff10",
    "list.activeSelectionForeground": "#bdc3c7",
    "list.inactiveSelectionForeground": "#bdc3c7",
    "list.hoverBackground": "#1A1A1A",

    /* Tabs */
    // "tab.activeBackground": "#0B0B0B"
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#ffffff10", //"#000000D0",
    "tab.inactiveForeground": "#ffffffB0",

    "scrollbar.shadow": "#000000",

    // "editorGroupHeader.tabsBorder": "#000000",
    "tab.border": "#1C1C1C",
    "editorGroupHeader.tabsBackground": "#1A1A1A",

    /* Widget */
    "editorHoverWidget.background": "#1C1C1C",
    "editorWidget.border": "#18181A",

    /* Editor match brackets */
    "editorBracketMatch.background": "#ffffff00",
    "editorBracketMatch.border": "#ffffff90",

    // Workbench: Status Bar
    "statusBar.foreground": "#ffffff90",
    "statusBar.background": "#18181A",
    "statusBar.noFolderBackground": "#595959",
    "statusBar.debuggingBackground": "#595959"
